{
  "name": "pinte_champagne",
  "version": "1.4.0",
  "author": "Richard DZK",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "lint": "eslint \"src/**\" --cache",
    "lint:fix": "eslint \"src/**\" --cache --fix",
    "ts-check": "tsc -p tsconfig.json --noEmit",
    "codegen": "npx apollo client:codegen --target=typescript",
    "schema:download": "npx apollo  schema:download --endpoint=http://localhost:4488/graphql/ graphql-schema.json",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run start",
    "build-mobile": "next build && next export && npx cap sync"
  },
  "dependencies": {
    "@apollo/client": "^3.5.5",
